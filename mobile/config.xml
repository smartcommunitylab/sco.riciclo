<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="130" id="it.smartcommunitylab.rifiuti.trento" version="1.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
